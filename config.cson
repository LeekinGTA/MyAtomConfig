"*":
  welcome:
    showOnStartup: false
  core:
    packagesWithKeymapsDisabled: [
      "tree-view-finder"
      "tool-bar"
      "python-yapf"
      "atom-ternjs"
      "url-encode"
      "git-control"
      "git-plus"
      "jsonlint"
      "build-tools"
      "glsl-preview"
      "video-player"
    ]
    followSymlinks: false
    disabledPackages: [
      "atom-react-native-css"
    ]
    themes: [
      "daylight-ui"
      "gl-light-syntax"
    ]
  editor:
    invisibles: {}
    scrollPastEnd: true
    fontFamily: "Consolas"
    showIndentGuide: true
    softWrap: true
    fontSize: 26
  minimap: {}
  "build-tools": {}
